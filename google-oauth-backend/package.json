{
  "name": "google-oauth-backend",
  "version": "1.0.0",
  "main": "dist/server.js",
  "scripts": {
    "dev": "nodemon",
    "clean": "rimraf dist",
    "build": "npm run clean && tsc",
    "build:prod": "npm run clean && npm run typecheck && esbuild src/server.ts --bundle --platform=node --target=node20 --outfile=dist/server.js --minify --sourcemap=external --packages=external",
    "start": "npm run build && node dist/server.js",
    "start:prod": "cross-env NODE_ENV=production npm run build:prod && node dist/server.js",
    "test:unit": "vitest",
    "lint": "eslint . --fix --cache",
    "lint:no-fix": "eslint . --max-warnings=0",
    "coverage": "vitest run --coverage",
    "vitest:ui": "vitest --ui --coverage.enabled=true",
    "typecheck": "tsc --noEmit",
    "test-lint-typecheck": "vitest run && npm run typecheck && npm run lint:no-fix"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "dependencies": {
    "axios": "^1.13.1",
    "cors": "^2.8.5",
    "dotenv": "^17.2.3",
    "express": "^5.1.0",
    "helmet": "^8.1.0",
    "http-proxy-middleware": "^3.0.5",
    "jsonwebtoken": "^9.0.2"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.1",
    "@stylistic/eslint-plugin": "^5.6.1",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.5",
    "@types/http-proxy-middleware": "^0.19.3",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "^24.9.2",
    "@types/supertest": "^6.0.3",
    "@typescript-eslint/eslint-plugin": "^8.47.0",
    "@typescript-eslint/parser": "^8.47.0",
    "@vitest/coverage-v8": "^4.0.9",
    "@vitest/eslint-plugin": "^1.4.3",
    "@vitest/ui": "^4.0.9",
    "cross-env": "^10.1.0",
    "esbuild": "^0.27.0",
    "eslint": "^9.39.1",
    "eslint-plugin-unused-imports": "^4.3.0",
    "globals": "^16.5.0",
    "jiti": "^2.6.1",
    "nodemon": "^3.1.10",
    "rimraf": "^6.1.2",
    "supertest": "^7.1.4",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.3",
    "vitest": "^4.0.8"
  }
}
