{
  "name": "sync-forge",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "engines": {
    "node": "^20.19.0 || >=22.12.0"
  },
  "scripts": {
    "dev": "vite",
    "build": "run-p type-check \"build-only {@}\" --",
    "build:watch": "vite build --watch",
    "preview": "vite preview --port 5173",
    "preview:watch": "run-p build:watch preview",
    "test:unit": "vitest",
    "test:e2e": "playwright test",
    "build-only": "vite build",
    "type-check": "vue-tsc --build",
    "lint": "eslint . --fix --cache",
    "lint:no-fix": "eslint . --max-warnings=0",
    "test-lint-typecheck": "vitest run && vue-tsc --build && npm run lint:no-fix",
    "coverage": "vitest run --coverage",
    "vitest:ui": "vitest --ui --coverage.enabled=true"
  },
  "dependencies": {
    "@tailwindcss/vite": "^4.1.16",
    "ace-builds": "^1.43.4",
    "axios": "^1.13.1",
    "i18next": "^25.6.0",
    "jodit": "^4.7.9",
    "js-beautify": "^1.15.4",
    "lucide-vue-next": "^0.554.0",
    "luxon": "^3.7.2",
    "pinia": "^3.0.3",
    "tailwindcss": "^4.1.16",
    "uuid": "^13.0.0",
    "vue": "^3.5.22",
    "vue-router": "^4.6.3"
  },
  "devDependencies": {
    "@pinia/testing": "^1.0.3",
    "@playwright/test": "^1.56.1",
    "@stylistic/eslint-plugin": "^5.6.1",
    "@tsconfig/node22": "^22.0.2",
    "@types/js-beautify": "^1.14.3",
    "@types/jsdom": "^27.0.0",
    "@types/luxon": "^3.7.1",
    "@types/node": "^22.18.11",
    "@vitejs/plugin-vue": "^6.0.1",
    "@vitest/coverage-v8": "^3.2.4",
    "@vitest/eslint-plugin": "^1.3.23",
    "@vitest/ui": "^3.2.4",
    "@vue/eslint-config-typescript": "^14.6.0",
    "@vue/test-utils": "^2.4.6",
    "@vue/tsconfig": "^0.8.1",
    "eslint": "^9.38.0",
    "eslint-plugin-playwright": "^2.2.2",
    "eslint-plugin-unused-imports": "^4.3.0",
    "eslint-plugin-vue": "~10.5.0",
    "happy-dom": "^20.0.10",
    "jiti": "^2.6.1",
    "jsdom": "^27.0.1",
    "msw": "^2.12.2",
    "npm-run-all2": "^8.0.4",
    "typescript": "~5.9.0",
    "vite": "^7.1.11",
    "vite-plugin-vue-devtools": "^8.0.3",
    "vitest": "^3.2.4",
    "vue-tsc": "^3.1.1"
  }
}
